import javax.swing.*;

public class Main
{
    public static void main(String[] args)
    {
        int number = Integer.parseInt(JOptionPane.showInputDialog("Please input a number!"));

        if (number % 2 == 0)
        {
            System.out.println("Even!");
        } else {
            System.out.println("Odd!");
        }
    }
}
